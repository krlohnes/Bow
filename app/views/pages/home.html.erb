<div class="parallax">
  <div class='content-wrapper'>
    <div id="group1" class="parallax__group">
    	<div class='star-wrapper'>
				<div id='stars-near'></div>
				<div id='stars-mid'></div>
				<div id='stars-far'></div>
			</div>
      <div class="parallax__layer parallax__layer--base">
      	<p class='center'>
      	<img src='/Bow_revised.gif' class='bow-title'/>
      </p>
	      <div class='bow_container'>
	      	<div class='bow'>
	      		<a href='/#music'><div class='bow__star bow__star-one'><div class='bow__star-title bow__star-title-left'><span>MUSIC</span></div></div></a>
						<a href='/#musicvideo'><div class='bow__star bow__star-two'><div class='bow__star-title bow__star-title-right'><span>VIDEO</span></div></div></a>
						<a href='/#live'><div class='bow__star bow__star-three'><div class='bow__star-title bow__star-title-right'><span>LIVE</span></div></div></a>
						<a href='/#social'><div class='bow__star bow__star-four'><div class='bow__star-title bow__star-title-left'><span>SOCIAL</span></div></div></a>
						<a href='/goldcoins' data-no-turbolink="true"><div class='bow__star bow__star-five'><div class='bow__star-title bow__star-title-left'><span>GOLD COINS</span></div></div></a>
						<img src="/BowConstellation-FINAL.svg" class='bow_constellation'/>
						<img src='/BowSillhouette.png' class='bow_silhouette'/>
					</div>
				</div>
      </div>
      <div class="parallax__layer parallax__layer--back">
      </div>
    </div>
    <div id="music" class="parallax__group">
      <div class="parallax__layer-rel parallax__layer--base">
      	<img src='/Header_BowMusic.png' class='type-header'/>
      	<div class='row container albums'>
      		<div class='col-md-6'>
      			<audio id='trackOne'>
      				<source src="BeYourBow.ogg" type="audio/ogg">
						  <source src="BeYourBow.mp3" type="audio/mpeg">
							Your browser does not support the audio element.
						</audio>
    				<div id='beYourBow' class='albums__coverart center'>
							<img src='/BeYourBowCoverArt.png' class='img-responsive'/>
							<a><div id='beYourBowInner' class='albums__coverart-inner'>
								<div class='albums__coverart-text'>
									<h2>Be Your Bow</h2>
									<img id='trackOnePlay' src='/PlayButton.png' class='play--button-music center'/>
									<img id='trackOnePause' src='/PauseButton.png' class='pause--button-music center'/>
								</div>
							</div></a>
						</div>
					</div>
					<div class='col-md-6'>
						<audio id='trackTwo'>
						  <source src="/Lovesound.ogg" type="audio/ogg">
						  <source src="/Lovesound.mp3" type="audio/mpeg">
							Your browser does not support the audio element.
						</audio>
						<div id='lovesound' class='albums__coverart center'>
							<img src='/LovesoundCoverArt.png' class='img-responsive'/>
							<a><div id='lovesoundInner' class='albums__coverart-inner'>
								<div class='albums__coverart-text'>
									<h2>Lovesound</h2>
									<img id='trackTwoPlay' src='/PlayButton.png' class='play--button-music center'/>
									<img id='trackTwoPause' src='/PauseButton.png' class='pause--button-music center'/>
								</div>
							</div></a>
						</div>
					</div>
				</div>
				<div class='albums-listen'>
					<img src='/Header_Listen.png' class='type-subheader'/>
					<div class='albums__logos center'>
						<a href='https://open.spotify.com/artist/1BygGOp0eZirObthaawEs7' target='_blank'><img src='/Spotify_Logo(400).png'/></a>
						<a href='https://itunes.apple.com/us/artist/bow/id1001685633' target='_blank'><img src='/Apple_Logo(400).png'/></a>
						<a href='https://www.youtube.com/user/LizLohnes' target='_blank'><img src='/YouTube_Logo(400).png'/></a>
					</div>
				</div>
      </div>
    </div>

    <div id="musicvideo" class="parallax__group">
      <div class="parallax__layer parallax__layer--base">
        <div class="title">
        	<img src='Header_Lovesound(Alt).png' class='type-header'/>
        	<a id='lovesoundOpen'><img src='/PlayButton.png' class='play--button-video center'/></a>
        </div>
      </div>
      <div class="parallax__layer parallax__layer--back">
        <div class='video-wrapper'>
        	<div id="bgimg">
        		<img src='/LovesoundStill.png'/>
        	</div>
					<video id="bgvid" muted="muted" volume="0" preload="auto" autoplay="autoplay" loop="loop">
					  <source src="/LovesoundMV.mp4" type="video/webm">
					  <source src="/SLovesoundMV.mp4" type="video/ogv">
					  <source src="/LovesoundMV.mp4" type="video/mp4">
					  Sorry, your browser does not support HTML5 video.
					</video>
				</div>
      </div>
    </div>
    <div id="live" class="parallax__group">
      <div class="parallax__layer parallax__layer--base">
        <img src='Header_BowLive(Alt).png' class='type-header'/>
	      <div class='live__dates container'> 

        <% if user_signed_in? %>
        	<%= link_to new_show_path, class: 'new-show pull-right' do%>
        	<span class='glyphicon glyphicon-plus-sign'></span> NEW SHOW
        	<% end %>
        <% end %>
	      	<table class='table table-condensed event-table'>
	      		<thead>
	      			<tr class='script-table-header'>
	      				<th>Date</th>
	      				<th>Venue</th>
	      				<th>City</th>
	      				<th>Tickets</th>
	      				<% if user_signed_in? %>
	      					<th></th>
	      				<% end %>
	      			</tr>
	      		</thead>
	      		<tbody>
	      			<% @shows.each do |show| %>
		      			<tr class='event-row '>
			      			<td><%= show.start_time.strftime("%b %d %I:%M %p") %></td>
			      			<td><%= show.venue %></td>
			      			<td><%= show.city %></td>
			      			<td><%= link_to "Tickets", show.ticket_link %></td>
			      			<% if user_signed_in? %>
			      				<td>
			      					<%= link_to edit_show_path(show) do%>
			      					<span class='glyphicon glyphicon-edit'></span>
			      					<% end %>
			      					<%= link_to show, method: :delete, data: { confirm: 'Are you sure?' } do%>
			      					<span class='glyphicon glyphicon-trash'></span>
			      					<% end %>
			      				</td>
			      			<% end %>
			      		</tr>
		      		<% end%>
	      		</tbody>
	      	</table>
	      </div>
      </div>
    </div>
    <div id="social" class="parallax__group">
      <div class="parallax__layer parallax__layer--base">
        <img src='Header_BowSocial(MultiAlt).png' class='type-header'/>
        <div class='social-icons center'>
        	<img src='/Facebook.png' />
        	<img src='/Twitter.png' />
        	<img src='/Instagram.png' />
        	<img src='/GooglePlus.png' />
        </div>
        <div class='center contact'>
        	<!--<form>
			    <input type='text'class="contact-input"/>
			    </form>-->
			  </div>
        <div class='row'>
        	<div class='col-md-6 center'>
        		<img src='Header_Instagram.png' class='type-subheader'/>
	        	<script type="text/javascript">
						     var userFeed = new Instafeed({
						        get: 'user',
						        userId: 1376567636,
						        accessToken: '1376567636.71f0ebd.b2d3dd4da3a74c6484cf11f5734d1984',
						        limit: 6
						    });
						    userFeed.run();
						</script>
						<div id="instafeed" class='social_feed-instagram center'></div>
					</div>
	        <div class='col-md-6 center'>
	        	<img src='Header_Facebook.png' class='type-subheader'/>
						<div class="fb-page" data-href="https://www.facebook.com/pages/Bow/12012275279" data-small-header="false" data-adapt-container-width="true" data-hide-cover="false" data-show-facepile="true" data-show-posts="false">
							<div class="fb-xfbml-parse-ignore">
								<blockquote cite="https://www.facebook.com/pages/Bow/12012275279"><a href="https://www.facebook.com/pages/Bow/12012275279">Bow</a></blockquote>
							</div>
						</div>
					</div>
				</div>
      </div>
    </div>
  </div>
</div>